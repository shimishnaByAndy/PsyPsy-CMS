    "cross-env": "^7.0.3",
    "rimraf": "^5.0.0",
    
    "axe-core": "^4.8.0",
    "@axe-core/playwright": "^4.8.0",
    
    "lighthouse": "^11.4.0",
    "@lhci/cli": "^0.13.0",
    
    "dotenv": "^16.3.0"
  },
  "simple-git-hooks": {
    "pre-commit": "lint-staged",
    "pre-push": "npm run type-check"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,css,md,json}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "healthcare",
    "psychology",
    "cms",
    "tauri",
    "desktop-application",
    "react",
    "typescript",
    "hipaa-compliant",
    "testing",
    "quality-assurance"
  ],
