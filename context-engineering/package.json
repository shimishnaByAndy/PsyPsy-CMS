{
  "name": "psypsy-cms-tauri",
  "version": "2.0.0",
  "description": "PsyPsy CMS Desktop Application - Healthcare Management System with Comprehensive Testing",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    
    "test": "npm run test:unit",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    
    "test:security": "playwright test tests/security/",
    "test:performance": "playwright test tests/performance/",
    "test:healthcare": "playwright test tests/e2e/healthcare-workflows.spec.ts",
    "test:hipaa": "playwright test tests/security/hipaa-compliance.spec.ts",
    
    "test:visual": "playwright test --config=playwright.visual.config.ts",
    "test:memory": "playwright test tests/performance/ --grep=\"memory\"",
    "test:load": "playwright test tests/performance/load-testing.spec.ts",
    
    "test:all": "npm run test:unit && npm run test:e2e && npm run test:security && npm run test:performance",
    "test:ci": "npm run test:coverage && npm run test:e2e -- --reporter=json",
    
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "type-check": "tsc --noEmit",
    
    "analyze:bundle": "vite-bundle-analyzer dist/",
    "analyze:deps": "madge --circular --extensions ts,tsx src/",
    
    "audit:security": "npm audit --audit-level moderate",
    "audit:licenses": "license-checker --summary",
    
    "db:start": "echo 'Starting test database...'",
    "db:seed": "echo 'Seeding test data...'",
    "db:reset": "echo 'Resetting test database...'",
    
    "docs:generate": "typedoc --out docs src/",
    "docs:serve": "serve docs/",
    
    "pre-commit": "lint-staged",
    "postinstall": "simple-git-hooks"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.0",
    "@tauri-apps/plugin-store": "^2.0.0",
    "@tauri-apps/plugin-window-state": "^2.0.0",
    "@tauri-apps/plugin-notification": "^2.0.0",
    "@tauri-apps/plugin-fs": "^2.0.0",
    "@tauri-apps/plugin-dialog": "^2.0.0",
    "@tauri-apps/plugin-clipboard-manager": "^2.0.0",
    
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    
    "@tanstack/react-query": "^5.0.0",
    "@tanstack/react-query-devtools": "^5.0.0",
    "@tanstack/react-table": "^8.10.0",
    
    "firebase": "^10.7.0",
    "firebase-admin": "^11.11.0",
    
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-button": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-form": "^0.0.3",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-table": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    
    "lucide-react": "^0.294.0",
    "framer-motion": "^10.16.0",
    "recharts": "^2.8.0",
    "react-big-calendar": "^1.8.5",
    "react-hook-form": "^7.48.0",
    "@hookform/resolvers": "^3.3.0",
    "zod": "^3.22.0",
    
    "date-fns": "^2.30.0",
    "react-datepicker": "^4.25.0",
    
    "i18next": "^23.7.0",
    "react-i18next": "^13.5.0",
    "i18next-browser-languagedetector": "^7.2.0",
    
    "@sentry/react": "^7.81.0",
    "@sentry/tauri": "^7.81.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.0.0",
    
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/node": "^20.9.0",
    
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    
    "typescript": "^5.2.0",
    
    "@vitejs/plugin-react": "^4.2.0",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.17.0",
    "vite-bundle-analyzer": "^0.7.0",
    
    "vitest": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    
    "@testing-library/react": "^14.1.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/user-event": "^14.5.0",
    "jsdom": "^23.0.0",
    
    "@playwright/test": "^1.40.0",
    
    "msw": "^2.0.0",
    "whatwg-fetch": "^3.6.0",
    
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    
    "lint-staged": "^15.2.0",
    "simple-git-hooks": "^2.9.0",
    
    "madge": "^6.1.0",
    "license-checker": "^25.0.1",
    "typedoc": "^0.25.0",
    "serve": "^14.2.0",
    
