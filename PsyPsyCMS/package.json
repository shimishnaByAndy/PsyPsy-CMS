{
  "name": "psypsy-cms-tauri",
  "version": "2.0.0",
  "description": "PsyPsy CMS - Modern Tauri Desktop Application",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:check": "tsc && vite build",
    "build:prod": "npm run type-check && npm run lint && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "npm run build:prod && tauri build",
    "tauri:build:debug": "npm run build:check && tauri build --debug",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:accessibility": "vitest --run tests/accessibility/",
    "test:a11y": "npm run test:accessibility",
    "test:all": "npm run test:coverage && npm run e2e && npm run test:accessibility",
    "e2e": "playwright test",
    "e2e:accessibility": "playwright test tests/e2e/accessibility/",
    "accessibility:validate": "npm run test:accessibility && npm run e2e:accessibility",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "quality": "npm run type-check && npm run lint && npm run test:coverage",
    "switch:dev": "node scripts/switch-environment.js dev",
    "switch:prod": "node scripts/switch-environment.js prod",
    "switch:status": "node scripts/switch-environment.js status",
    "test:firebase": "node scripts/test-firebase-connection.js",
    "debug:tauri": "tauri dev --debug",
    "debug:webview": "tauri dev --debug --port 1420",
    "setup:dev": "npm run switch:dev && npm run test:firebase",
    "setup:prod": "npm run switch:prod"
  },
  "dependencies": {
    "@axe-core/react": "^4.10.2",
    "@hookform/resolvers": "^3.3.4",
    "@nextui-org/react": "^2.6.11",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-query": "^5.89.0",
    "@tanstack/react-query-devtools": "^5.20.0",
    "@tanstack/react-query-persist-client": "^5.87.4",
    "@tanstack/react-table": "^8.21.3",
    "@tauri-apps/api": "^2.0.0",
    "@tauri-apps/plugin-shell": "^2.0.0",
    "@types/file-saver": "^2.0.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "file-saver": "^2.0.5",
    "firebase": "^12.2.1",
    "firebase-admin": "^13.5.0",
    "framer-motion": "^11.0.0",
    "i18next": "^23.8.0",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.4.0",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-big-calendar": "^1.10.0",
    "react-dom": "^19.1.1",
    "react-dropzone": "^14.2.0",
    "react-hook-form": "^7.49.0",
    "react-i18next": "^14.0.0",
    "react-router-dom": "^6.21.0",
    "recharts": "^2.10.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "^1.41.0",
    "@tauri-apps/cli": "^2.0.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/node": "^20.11.0",
    "@types/react": "^19.1.13",
    "@types/react-big-calendar": "^1.8.0",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest-axe": "^10.0.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.2.0"
  }
}