# PsyPsy CMS Environment Variables

# === FIREBASE CONFIGURATION ===
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=123456789
REACT_APP_FIREBASE_APP_ID=1:123456789:web:abcdef123456

# === PARSE SERVER CONFIGURATION (Legacy) ===
REACT_APP_PARSE_APP_ID=your_parse_app_id
REACT_APP_PARSE_JAVASCRIPT_KEY=your_parse_javascript_key
REACT_APP_PARSE_SERVER_URL=https://your-parse-server-url.com/parse

# === SOCIAL MEDIA INTEGRATION ===

# Postiz API Configuration
# Get your API key from: https://docs.postiz.com/public-api
REACT_APP_POSTIZ_API_URL=https://api.postiz.com
REACT_APP_POSTIZ_API_KEY=your_postiz_api_key_here

# === AI CONTENT GENERATION ===

# Google Gemini API (Nano Banana - Image Generation)
# Get your API key from: https://ai.google.dev/
REACT_APP_GEMINI_API_KEY=your_gemini_api_key_here

# Google Vertex AI (Veo3 - Video Generation)
# Requires Google Cloud Project with Vertex AI enabled
REACT_APP_GOOGLE_PROJECT_ID=your-google-cloud-project-id
REACT_APP_VERTEX_AI_TOKEN=your_vertex_ai_access_token

# === DEVELOPMENT CONFIGURATION ===
REACT_APP_ENV=development
REACT_APP_DEBUG=true

# === ANALYTICS & MONITORING ===
REACT_APP_ANALYTICS_ID=your_analytics_id
REACT_APP_SENTRY_DSN=your_sentry_dsn

# === ELECTRON CONFIGURATION ===
ELECTRON_IS_DEV=true
ELECTRON_DEBUG=true