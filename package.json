{
  "name": "psypsy-cms",
  "version": "1.0.0",
  "main": "electron/main.js",
  "homepage": "./",
  "scripts": {
    "start": "npx cross-env GENERATE_SOURCEMAP=false PORT=3022 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:dev": "concurrently \"npx cross-env BROWSER=none PORT=3022 npm start\" \"sleep 15 && npx cross-env ELECTRON_DISABLE_SECURITY_WARNINGS=true NODE_ENV=development electron .\"",
    "electron:dev-alt": "concurrently --kill-others --success first \"npx cross-env BROWSER=none PORT=3022 npm start\" \"wait-on http://localhost:3022 && npx cross-env ELECTRON_DISABLE_SECURITY_WARNINGS=true NODE_ENV=development electron .\"",
    "electron:restart": "npx cross-env ELECTRON_DISABLE_SECURITY_WARNINGS=true NODE_ENV=development electron .",
    "electron:build": "npm run build && electron-builder",
    "package": "electron-builder build --mac --win --linux -c.extraMetadata.main=electron/main.js --publish never",
    "create-admin": "node scripts/createAdmin.js",
    "sync-i18n": "node scripts/sync-i18n.js"
  },
  "build": {
    "appId": "com.psypsy.cms",
    "productName": "PsyPsy CMS",
    "files": [
      "build/**/*",
      "electron/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "PsyPsy CMS based on Material Dashboard React",
  "dependencies": {
    "@emotion/cache": "^11.10.8",
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@iconify/react": "^5.2.1",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.3",
    "@mui/system": "^5.12.3",
    "@mui/x-data-grid": "^6.19.11",
    "apexcharts": "^4.2.0",
    "bootstrap": "^5.3.3",
    "chart.js": "^4.3.0",
    "chroma-js": "^2.4.2",
    "clsx": "^2.1.1",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^36.0.0",
    "electron-builder": "^26.0.12",
    "flatpickr": "^4.6.13",
    "framer-motion": "^12.15.0",
    "gridjs": "^6.2.0",
    "gridjs-react": "^6.1.1",
    "healthicons-react": "^3.2.5",
    "i18next": "^25.0.2",
    "i18next-browser-languagedetector": "^8.0.5",
    "jsvectormap": "^1.3.2",
    "mcp-shrimp-task-manager": "^1.0.19",
    "moment": "^2.30.1",
    "motion": "^12.15.0",
    "parse": "^6.1.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-apexcharts": "^1.7.0",
    "react-bootstrap": "^2.10.6",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-flatpickr": "^4.0.10",
    "react-github-btn": "^1.4.0",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.5.1",
    "react-router-dom": "^6.11.0",
    "react-scripts": "^5.0.1",
    "react-table": "^7.8.0",
    "simplebar-react": "^3.3.0",
    "stylis": "^4.1.4",
    "stylis-plugin-rtl": "^2.1.1",
    "tailwind-merge": "^3.3.0",
    "wait-on": "^8.0.3",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "electron-reloader": "^1.2.3",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~4.9.5"
  }
}
